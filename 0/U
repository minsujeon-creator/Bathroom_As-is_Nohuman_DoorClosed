FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    walls
    {
        type            noSlip;
    }
    doorClosed
    {
        type            noSlip;
    }
    door
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    vent
    {
        type            fixedValue;
        value           uniform (0 0 0.506); // 73CMH through 0.2m x 0.2m: (73/3600)/(0.2*0.2) â‰ˆ 0.506 m/s
    }
    room
    {
        type            noSlip;
    }

    /*toilet
    {
        type            noSlip;
    }*/
    /*human
    {
        type            noSlip;
    }
    floor
    {
        type            noSlip;
    }
    symmCut
    {
        type            symmetryPlane;
    }*/
}


